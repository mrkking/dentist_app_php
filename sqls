CREATE TABLE `dental_test`.`user` ( `id` INT NOT NULL AUTO_INCREMENT , `email` VARCHAR NULL , `password` VARCHAR NOT NULL , `first_name` VARCHAR NOT NULL , `last_name` VARCHAR NOT NULL , `age` INT NOT NULL , `date_of_birth` DATE NOT NULL , `insurance_name` VARCHAR NOT NULL , `policy_number` VARCHAR NOT NULL , `card_front_img` VARCHAR NOT NULL , `card_back_img` VARCHAR NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;
CREATE TABLE `dental_test`. ( `id` INT NOT NULL AUTO_INCREMENT , `user` INT NOT NULL , `date` DATE NOT NULL , `time` TIMESTAMP NOT NULL , `visit_reason` VARCHAR(255) NOT NULL , PRIMARY KEY (`id`), INDEX `user` (`date`)) ENGINE = InnoDB;